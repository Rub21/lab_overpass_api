/* Toolbar Tooltip */
a.tooltip, a.tooltip:link, a.tooltip:visited, a.tooltip:active  {
  position: relative;
  text-decoration: none; 
  }
  
a.tooltip:hover {
  background: transparent;
  }

a.tooltip span {
  display: none;  
  text-decoration: none; 
}

a.tooltip:hover span {
	display: block;
	position: absolute;
	z-index: 100;
	color: #002380;
	border:1px solid #002380;
	background: #EDF1F4;
	font: .9em 'Lucida Grande', Verdana, Arial, Sans-Serif;
	top: -20px;
	cursor: crosshair;
	padding: 5px;
  }

/* End Toolbar Tooltip */
